.cplb_reservation {
	width: 1280px;
	height: 1024px;
	margin: 0 auto;
	background-color: #FFF;
	position: relative;
}

.submit_btn {
	width: 420px;
	height: 70px;
	background: url(../img/hjw_login_btn.png) no-repeat;
	color: #333;
	font-size: 30px !important;
	border: 0px;
}

.return_btn {
	background: url(../img/hjw_return_bg.png) no-repeat;
	text-align: center;
	position: absolute;
	bottom: 60px;
	right: 60px;
	width: 100px;
	height: 100px;
	color: #FDD017;
	font-size: 30px;
	line-height: 100px;
}