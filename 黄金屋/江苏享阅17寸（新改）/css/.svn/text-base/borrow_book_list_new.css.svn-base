.cplb_r1 {
	float: right;
	width: 1280px;
	height: 800px;
}

.yzm_sr {
	float: left;
	width: 410px;
	height: auto;
	margin: 50px 0 0 440px;
}

.yzm_sr h3 {
	line-height: 70px;
	font-size: 30px;
	color: #333333;
}

.yzm_sr .in1 {
	width: 410px;
	border-radius: 5px;
	background: #f3f3f3;
	height: 68px;
	line-height: 68px;
	text-indent: 10px;
	font-size: 24px;
	border: 1px solid #d4a7a5;
}

.cplb_r1{
	margin-left: 70px;
}

.cplb_r1 .yzm_jp {
	margin-right: 50px;
}

.yzm_jp {
	float: right;
	width: 408px;
	margin: 194px 204px 0 0
}

.yzm_jp ul li {
	float: left;
	width: 102px;
	height: 102px;
	text-align: center;
	line-height: 94px;
	border-radius: 47px;
	margin: 0 10px 16px 10px;
	font-size: 46px;
	color: #53565c;
}

.yzm_jp ul li a {
	display: block;
	color: #5c5c5c;
	width: 100%;
	height: 102px;
	background: url(../images/jp_li_bj.png);
}

.yzm_jp ul li a:hover {
	background: url(../images/jp_li_bj1.png)
}

.yzm_jp ul li.qk_biao a {
	background: url(../images/jp_qk_biao-.png);
	font-size: 32px;
	font-family: "微软雅黑";
}

.yzm_jp ul li.qk_biao a:hover {
	background: url(../images/jp_qk_biao1-.png);
}

.yzm_jp ul li.qd_biao a {
	background: url(../images/jp_qd_biao-.png);
	font-size: 32px;
	font-family: "微软雅黑";
}

.yzm_jp ul li.qd_biao a:hover {
	background: url(../images/jp_qd_biao1-.png);
}

.yzm_jp ul li.qk_biao a,
.yzm_jp ul li.qd_biao a {
	color: #fff;
	font-size: 26px;
}
