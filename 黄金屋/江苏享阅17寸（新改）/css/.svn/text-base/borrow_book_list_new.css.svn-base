.booklistHeader {
	width: 300px;
	height: 60px;
	border: 2px solid #333;
	border-radius: 40px;
	margin: 0px auto;
}

.yellow_l {
	background: url(../img/hjw_black_bg.png) left center no-repeat;
}

.yellow_r {
	background: url(../img/hjw_black_bg.png) right center no-repeat;
}

.booklistHeader span {
	float: left;
	width: 150px;
	text-align: center;
	line-height: 60px;
	font-size: 36px;
	font-family: "微软雅黑";
	color: #333;
}

.timeTask {
	width: 100px;
	position: fixed;
	text-align: right;
	font-size: 36px;
	top: 30px;
	right: 40px;
}

.over-x {
	width: 1280px;
	height: 80px;
	overflow-x: scroll;
	overflow-y: hidden;
}

.on-book-type {
	border-bottom: 2px solid #FDD017;
	color: #333;
	font-size: 30px;
	padding-bottom: 17px;
	height: 80px;
	line-height: 80px;
	margin-right: 80px;
}

.off-book-type{
	color: #999;
	font-size: 30px;
	height: 80px;
	line-height: 80px;
	margin-right: 80px;
}

.booklistDl {
	list-style: none;
	width: 2160px;
	padding-left: 60px;
	overflow-x: scroll;
	overflow-y: hidden;
}

.over-x::-webkit-scrollbar,
.booklistDl::-webkit-scrollbar,
.ulbox::-webkit-scrollbar {
	display: none;
}

.cplb_r {
	width: 1280px;
	height: 800px !important;
	position: relative
}

.cplb_r ul {
	width: 1280px;
	padding-left: 80px;
}

.book_item{
	float: left;
	width: 150px;
	height: 250px;
	margin: 50px 90px 0px 0px;
	background: #fff;
	position: relative
}

.cplb_text {
	text-align: center;
	margin-top: 20px;
	padding: 8px 22px;
	line-height: 23px;
	font-size: 23px;
	color: #FDD017;
	background-color: #333;
	border-radius: 30px;
}

.bookimg{
	width: 142px;
	height: 187px;
	-moz-box-shadow: 3px 3px 5px #888888; /* 老的 Firefox */
	box-shadow: 3px 3px 5px #888888;
}

.fy {
	text-align: center;
	width: auto;
	margin: 0px auto;
	overflow: hidden;
}

.fy span {
	display: inline-block;
	margin: 0 15px;
	float: left;
}

.fy span a {
	display: inline-block;
	width: 100%;
	height: 100%;
}

.fy span.fy_left {
	background: url(../img/hjw_icon_retreat.png) no-repeat;
	background-size: 56px 56px;
	width: 56px;
	height: 56px;
	font-size: 0;
	overflow: hidden;
	margin-top: 15px;
}

.fy span.fy_right {
	background: url(../img/hjw_icon_forward.png) no-repeat;
	background-size: 56px 56px;
	width: 56px;
	height: 56px;
	font-size: 0;
	overflow: hidden;
	margin-top: 15px;
}

.fy span.fy_center {
	background: url(../img/hjw_icon_pages.png) no-repeat;
	background-size: 86px 86px;
	width: 86px;
	height: 86px;
	text-align: center;
	line-height: 86px;
	font-size: 32px;
	color: #333;
}

