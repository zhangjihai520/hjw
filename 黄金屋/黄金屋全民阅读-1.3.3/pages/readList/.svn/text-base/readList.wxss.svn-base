.listitem{
  width: 100%;
  height: 40px;
  border-bottom: 5px solid #F3F3F3;
}

.article_name{
  height: 40px;
  line-height: 40px;
  font-size: 15px;
  padding-left: 10px;
  float: left;
}

.bgmusic_name{
  height: 48px;
}

.b1 {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.4);
  overflow: hidden;
}

.b2 {
  width: 250px;
  height: 130px;
  margin: 45% auto;
  overflow: hidden;
  background-color: #fff;
  border-radius: 12rpx;
}

.title{
  width: 100%;
  height: 20px;
  margin-top: 10px;
  font-size: 16px;
  text-align: center;
  line-height: 20px;
}

.submit{
  width: 100%;
  height: 40px;
  background-color: #FDD017;
  text-align: center;
  line-height:40px;
  font-size: 12px;
  color: #333;
  float: left;
}

.inputScore{
  width: 94%;
  margin: 15px 3%;
  height: 30px;
  font-size: 12px;
  background-color: #F3F3F3;
}

.scoreText{
  height: 40px;
  float: right;
  font-size: 12px;
  line-height: 40px;
  margin-right: 10px;
}

.scoreBtn{
  width: 75px;
  height: 30px;
  float: right;
  font-size: 15px;
  text-align: center;
  line-height: 30px;
  background-color: #FDD017;
  border-radius: 10px;
  margin: 4px 10px;
}