.sinput {
	width: 100%;
	height: 71px;
}

.sinput div.stxt {
	width: 608px;
	height: 67px;
	line-height: 67px;
	text-indent: 15px;
	border: 1px #e4e4e4 solid;
	font-size: 26px;
	color: #cecece;
	border-radius: 10px;
	float: left;
