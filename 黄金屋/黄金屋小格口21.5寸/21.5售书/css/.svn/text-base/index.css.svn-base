/*硬件测试*/
.bin{
	width: 250px;
	height: 54px;
	line-height: 54px;
	float:left;
	text-align: center;
	border-bottom: 2px #D5D5D5 solid;
	font-size: 30px;
}

.bin:empty::before{
	content:attr(placeholder);
	color:#D5D5D5;
	line-height: 54px;
	font-size: 30px;

}
.openBoxs{
	border: none;
	background: #FDD017;
	width: 120px;
	height: 60px;
	float: left;
	margin-left: 80px;
	border-radius: 12px;
	font-size: 30px;
	font-weight: 500;
	color: #333;
	text-align: center;
	line-height: 60px;
}
.lightBox{
	width:100px;
	height: 100px;
	border-radius: 50%;
	float: left;
	
}
.lightBox:nth-of-type(1){
	background: url(../img/light00.png);
}
.lightBox:nth-of-type(2){
	background: url(../img/light01.png);
	margin:0 180px;
}
.lightBox:nth-of-type(3){
	background: url(../img/restart.png)
}
