.boxShowLine {
	height: 12px;
	width: 100%;
	background: url(../images/showLine.png)
}
.network {
	width: 50px;
	height: 50px;
	margin-top: 25px;
	float: right;
}
.sildeMiddle {
	height: 180px;
	width: 100%;
	background: #FDD017;
	position: absolute;
}

.sildeMiddle p {
	overflow: hidden;
	font-family: simsun;
	font-size: 36px;
	color: #333;
}

.sildeMiddle .d01_l {
	margin: 20px 0 0 20px;
	float: left;
	color: #FFF;
}

.sildeMiddle .d01_r {
	margin: 20px 20px 0 20px;
	font-size: 50px;
	float: right;
		color: #FFF;
}

.sildeMiddle .d02_l {
	margin: 20px 0 0 10px;
	float: left;
	color: #FFF;
}

.sildeMiddle .d02_r {
	margin: 20px 20px 0 20px;
	float: right;
	color: #FFF;
}

.sildeMiddle .d02_r a {
	margin-left: 45px;
}

