  padding-top: 20px;
  float: left;
}

.progressName{
  font-size: 15px;
}

.view1{
  width: 14%;
  height: 75px;
  float: left;
  padding-top: 30px;
}

.progressView1{
  width: 100%;
  height: 1px;
  background-color: #1edd19;
}

.progressView2{
  width: 100%;
  height: 1px;
  background-color: #999;
}

.line{
  width: 100%;
  height: 1px;
  background-color: #F3F3F3;
  margin-top: 10px;
  float: left;
}

.describe{
  width: 84%;
  font-size: 12px;
  margin-left: 3%;
  margin-bottom: 10px;
  margin-top: -20px;
  color: #666;
  float: left;
}

.price{
  width: 13%;
  font-size: 12px;
  margin-top: -20px;
  color: #FDD017;
  float: right;
}

.service_describe{
  width: 84%;
  font-size: 12px;
  margin-left: 3%;
  margin-bottom: 10px;
  margin-top: -8px;
  color: #666;
  float: left;
}

.service_price{
  width: 13%;
  font-size: 12px;
  margin-top: -8px;
  color: #FDD017;
  float: right;
}

.text{
  width: 100%;
  font-size: 10px;
  color: #999;
  margin-left: 10px;
  float: left;
}

.star_title{
  width: 100%;
  font-size: 15px;
  color: #333;
  float: left;
  margin: 10px;
}

.item_view{
  width: 100%;
  min-height: 45px;
  float: left;
}

.star_item_unselect{
  width: 75px;
  height: 35px;
  border: 1px #999 solid;
  float: left;
  font-size: 12px;
  color: #999;
  border-radius: 8px;
  text-align: center;
  padding: 3px 0px;
  margin: 0px 0px 10px 10px;
}

.star_item_select{
  width: 75px;
  height: 35px;
  float: left;
  font-size: 12px;
  border: 1px #FDD017 solid;
  background-color: #FDD017;
  color: #333;
  border-radius: 8px;
  text-align: center;
  padding: 3px 0px;
  margin: 0px 0px 10px 10px;
}



.rechageBtn{
  width: 200px;
  height: 35px;
  text-align: center;
  line-height: 35px;
  color: #333;
  font-size: 15px;
  background-color: #FDD017;
  float: left;
  border-radius: 8px;
  margin: 20px 23%;
}