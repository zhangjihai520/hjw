.item{
  width: 100%;
  min-height: 44px;
  line-height: 44px;
  font-size: 15px;
  border-bottom: 1px #F3F3F3 solid;
}

.itemName{
  width: 22%;
  color: #333;
  float: left;
  padding-left: 2%;
}

.itemContent{
  width: 74%;
  color: #333;
  float: right;
  text-align: right;
  padding-right: 2%;
}

.picker{
  width: 74%;
  height: 44px;
  color: #333;
  float: right;
  text-align: right;
  padding-right: 2%;
}

.itemAddress{
  width: 74%;
  color: #333;
  float: right;
  padding-right: 2%;
}

.submit{
  width: 200px;
  height: 47px;
  border-radius: 10px;
  background-color: #FDD017;
  text-align: center;
  line-height: 47px;
  font-size: 14px;
  margin: 30px auto;
}

.box1{
  width: 100%;
  height: 100px;
  background-color: #FFF;
  border-top: 5px #F3F3F3 solid;
  border-bottom: 5px #F3F3F3 solid;
}

.box2{
  width: 100%;
  height: 100px;
  background-color: #FFF;
}

.bookImg{
  width: 75px;
  height: 80px;
  margin: 10px;
  float: left;
}

.bookDetail{
  width: 70%;
  float: left;
}

.bookName{
  font-size: 12px;
  color: #333;
  margin: 10px 0px 16px 0px;
}

.reservatime{
  font-size: 12px;
  color: #333;
  margin-bottom: 4px;
}

.address{
  font-size: 12px;
  color: #333;
}

.notice{
  font-size: 12px;
  color: #333;
  text-align: center;
  margin: 0px auto;
  padding-top: 10px;
}

.code{
  width: 150px;
  height: 35px;
  font-size: 15px;
  line-height: 35px;
  text-align: center;
  color: #333;
  margin:20px auto;
  background-color: #FDD017;
}