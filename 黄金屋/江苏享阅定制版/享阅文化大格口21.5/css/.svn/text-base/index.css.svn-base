.whole {
	width: 1080px;
	height: 1920px;
	margin: 0 auto;
	position: relative;
	background: -webkit-linear-gradient(#f4f4f4, #d6e8ea);
	/* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(#f4f4f4, #d6e8ea);
	/* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(#f4f4f4, #d6e8ea);
	/* Firefox 3.6 - 15 */
	background: linear-gradient(#f4f4f4, #d6e8ea);
	/* 标准的语法 */
	background: #fff !important;
	overflow: hidden
}

.top {
	position: absolute;
	top: 0;
