.searchcon {
	width: 800px;
	margin: 190px auto 0;
}

.sinput {
	width: 100%;
	height: 71px;
}

.sinput div.stxt {
	width: 608px;
	height: 67px;
	line-height: 67px;
	text-indent: 15px;
	border: 1px #e4e4e4 solid;
	font-size: 26px;
	color: #cecece;
	border-radius: 10px;
	float: left;
	color: #333333;
}
.sinput div.stxt:empty::before{
	content: attr(placeholder);
	color: #D5D5D5;
	font-size: 26px;
}
.sinput input.sbtn {
	width: 156px;
	height: 71px;
	background: url(../images/sbtnbg.png) no-repeat;
	border: 0 none;
	color: #fff;
	line-height: 60px;
	font-size: 28px;
	letter-spacing: 5px;
	float: right;
}

.bklist {
	width: 100%;
	height: 920px;
	margin: 70px 0;
	overflow: auto;
}

.bklist div {
	margin-bottom: 30px;
}

.bkpic {
	width: 214px;
	height: 255px;
	color: left;
}

.bklt {
	width: 550px;
	height: 135px;
	float: right;
	margin: 60px 0;
}

.bkname {
	color: #57a4df !important;
	font-size: 26px !important;
}

.bklt p {
	font-size: 24px;
	color: #333;
	line-height: 45px;
}


/*底部*/

.hjw_pagefoot {
	position: absolute;
	bottom: 60px;
	width: 968px;
	padding: 0 56px;
	height: 100px;
}

.hjw_pagefoot p {
	font-family: simsun;
	color: #573613;
	font-size: 24px;
}

.foot_bl {
	float: left;
	height: 100px;
	width: 61px;
	text-align: center;
}

.foot_bt img {
	margin-bottom: 14px;
	margin-top: 16px;
}

.foot_br {
	float: right;
	height: 100px;
	width: 61px;
	text-align: center;
}

.foot_bm {
	width: 280px;
	height: 100px;
	float: left;
	margin-left: 344px;
}

.foot_bm span {
	float: left;
}

.foot_bm .img_retreat {
	height: 60px;
	width: 60px;
	background: url(../images/icon_retreat.png) center no-repeat;
	margin-top: 20px;
}

.foot_bm .img_center {
	height: 90px;
	width: 90px;
	background: url(../images/icon_pages.png) center no-repeat;
	margin: 5px 34px;
	text-align: center;
	line-height: 90px;
	color: #FFF;
	font-size: 24px;
}

.foot_bm .img_forward {
	height: 60px;
	width: 60px;
	background: url(../images/icon_forward.png) center no-repeat;
	margin-top: 20px;
}
