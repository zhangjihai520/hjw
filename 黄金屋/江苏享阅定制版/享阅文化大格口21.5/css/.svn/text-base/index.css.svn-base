	float: right;
	z-index: 9999
}

.sbh_r a {
	color: #666;
	marign-right: 40px;
	padding: 12px 24px;
	border: 2px solid transparent;
}

.sbh_r a:hover,
.sbh_r a.on {
	border-radius: 3px;
	border: 2px solid #d6461b;
	color: #d6461b;
	padding: 12px 24px;
}


/* slidepic-01 */

.slidebox-01 {
	width: 100%;
	height: 900px;
	overflow: hidden;
	position: relative;
}

.slidepic-01 {
	position: absolute;
	width: 9999em;
	border-bottom: 6px solid rgb(44, 44, 44);
}


/* 必要元素 */

.slidepic-01 li {
	height: 750px;
	overflow: hidden;
	float: left;
}

.slidebox-01 img {
	vertical-align: middle;
}

.slidebtn-01 {
	position: absolute;
	bottom: 10px;
	right: 10px;
	float: right;
	display: none;
}

.slidebtn-01 li {
	border: 1px solid #D00000;
	cursor: pointer;
	float: left;
	font-family: arial;
	height: 18px;
	line-height: 18px;
	width: 18px;
	margin: 4px;
	text-align: center;
	color: #D00000;
}

.slidebtn-01 li.current {
	border: 1px solid #D00000;
	height: 24px;
	line-height: 24px;
	width: 24px;
	margin: 0 2px;
	color: #fff;
	font-weight: normal;
}

