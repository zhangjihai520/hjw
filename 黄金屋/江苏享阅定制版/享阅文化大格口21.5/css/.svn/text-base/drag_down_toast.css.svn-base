.menu-open {
	height: 100%;
	width: 100%;
}
.menu-open .mui-scroll-wrapper {
	position: absolute;
	top: 48;
	bottom: 0;
	left: 0;
	z-index: 1;
	width: 100%;
	overflow: hidden;
	-webkit-backface-visibility: hidden;
}



.menu-wrapper {
	position: absolute;
	top: 0px;
	left: 0;
	right: 0;
	z-index: 999;
	text-align: left;
	background-color: #fff;
	width: 100%;
}


.menu-wrapper.hidden {
	-webkit-transform: translate3d(0, -100%, 0);
	transform: translate3d(0, -100%, 0);
	z-index: 10;
}

.menu {
	width: 100%;
}
.menu .mui-table-view-inverted {
	color: #000000;
	font-size: 19px;
}
.menu .mui-table-view-inverted .mui-table-view-cell:after {
	height: 2px;
	left: 0;
	right: 0;
}
.menu-wrapper.mui-active,
.menu-wrapper.mui-active .menu {
	-webkit-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0);
}

.message-wrapper{
	height: 44px; 
	line-height: 44px; 
	padding-left: 10px;
	padding-right: 10px;
	margin: 0;
	background-color: #fff;
	color: #000000;
}

.message-icon{
	font-size: 21px;
	color: #00ce05;
}
